{
	"name": "tinker-this",
	"displayName": "Tinker This",
	"description": "Run the selected code with Laravel Tinker",
	"version": "0.2.4",
	"publisher": "carlboo",
	"icon": "icon.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/turez/vscode-tinker-this"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tinker-this.runSelection"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tinker-this.runSelection",
				"title": "Tinker this"
			}
		],
		"keybindings": [
			{
				"key": "cmd+k cmd+t",
				"command": "tinker-this.runSelection"
			}
		],
		"configuration": {
			"title": "Tinker This configuration",
			"properties": {
				"tinker-this.phpPath": {
					"type": [
						"string"
					],
					"default": "php",
					"description": "Php executable path in use for your project. Also compatible with docker as \"docker-compose exec -T php-fpm php\""
				}
			}
		},
		"taskDefinitions": [
			{
				"type": "tinker-this",
				"required": [
					"task"
				],
				"properties": {
					"task": {
						"type": "string",
						"description": "The task to execute"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.40.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
